    "next": "^12.3.1",
    "next-auth": "^4.13.0",
    "next-progress": "^2.2.0",
    "next-seo": "^5.5.0",
    "react": "^18.2.0",
    "react-countdown": "^2.3.2",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-leaflet": "^4.1.0",
    "react-range": "^1.8.14"
